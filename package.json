{
  "name": "@n3okill/tslinq",
  "version": "1.0.0",
  "description": "Linq in typescript",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/mjs/index.js",
  "exports": {
    ".": {
      "import": "./dist/mjs/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n3okill/tslinq.git"
  },
  "bugs": {
    "url": "https://github.com/n3okill/tslinq/issues"
  },
  "homepage": "https://github.com/n3okill/tslinq#readme",
  "scripts": {
    "setup": "npm install",
    "clean:dist": "rimraf ./dist",
    "build:mjs": "npx tsc -p tsconfig-mjs.json",
    "build:cjs": "npx tsc -p tsconfig-cjs.json",
    "build:package.json": "node ./build.js",
    "build": "npm run clean:dist && npm run build:mjs && npm run build:cjs && npm run build:package.json",
    "pretest": " npm run build",
    "unit": "mocha",
    "test": "npm run lint && npm run unit",
    "lint": "eslint src --ext .ts && eslint tests --ext .ts",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" \"tests/**/*.ts\" \"docs/**/*.md\" \"README.md\" --write",
    "prepublishOnly": "npm test && npm run lint",
    "prepare": "npm run build",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "benchmark": "npm run build && cd benchmarks && node ./benchmark.js",
    "typedoc": "typedoc --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@n3okill/utils": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "benny": "^3.7.1",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^25.3.0",
    "mocha": "^9.1.3",
    "prettier": "^2.5.1",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.8",
    "typescript": "^4.5.4"
  },
  "keywords": [
    "linq"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": {
    "name": "Jo√£o Parreira",
    "email": "joaofrparreira@gmail.com"
  },
  "license": "MIT"
}