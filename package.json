{
  "name": "@n3okill/tslinq",
  "version": "1.0.0",
  "description": "Linq in typescript",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/n3okill/tslinq.git"
  },
  "bugs": {
    "url": "https://github.com/n3okill/tslinq/issues"
  },
  "homepage": "https://github.com/n3okill/tslinq#readme",
  "scripts": {
    "build": "npx tsc -p tsconfig.json",
    "lint": "eslint",
    "test": "globstar -- node --import tsx --test \"tests/**/*.test.ts\"",
    "test:coverage": "globstar -- node --experimental-test-coverage --test-reporter=spec --test-reporter-destination=stdout --test-reporter=lcov --test-reporter-destination=lcov.info --import tsx --test \"tests/**/*.test.ts\"",
    "test:watch": "globstar -- node --import tsx --test --watch \"tests/**/*.test.ts\"",
    "format": "prettier --config .prettierrc \"src/**/*.ts\" \"tests/**/*.ts\" --write",
    "prepare": "npm run build",
    "preversion": "npm run lint && npm test",
    "version": "npm run format && git add -A .",
    "postversion": "git push && git push --tags",
    "benchmark": "npm run build && cd benchmarks && node ./benchmark.js"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@types/node": "^22.8.1",
    "benny": "^3.7.1",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-security": "^3.0.1",
    "globstar": "^1.0.0",
    "prettier": "^3.3.3",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.11.0"
  },
  "keywords": [
    "linq"
  ],
  "files": [
    "dist/**/*"
  ],
  "author": {
    "name": "Jo√£o Parreira",
    "email": "joaofrparreira@gmail.com"
  },
  "license": "MIT"
}
